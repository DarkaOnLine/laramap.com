{
    "name": "laramap/laramap.com",
    "description": "The Laramap.com source.",
    "keywords": ["laramap", "laravel", "community"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "algolia/algoliasearch-client-php": "^1.27",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^4.0",
        "firebase/php-jwt": "^5.0",
        "get-stream/stream-laravel": "^2.3",
        "laravel-notification-channels/twitter": "^1.0",
        "laravel/framework": "5.6.*",
        "laravel/horizon": "^1.3",
        "laravel/passport": "^6.0",
        "laravel/scout": "^5.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "pragmarx/countries": "^0.5.8",
        "pragmarx/countries-laravel": "^0.5.2",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "^3.1",
        "rennokki/befriended": "^1.1",
        "spatie/geocoder": "^3.3",
        "spatie/laravel-cors": "^1.2",
        "spatie/laravel-csp": "^1.3",
        "spatie/laravel-feed": "^2.1",
        "spatie/laravel-permission": "^2.12",
        "spatie/laravel-query-builder": "^1.10",
        "spatie/laravel-referer": "^1.3",
        "spatie/laravel-sluggable": "^2.1",
        "spatie/schema-org": "^2.0",
        "vinkla/hashids": "^5.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "beyondcode/laravel-er-diagram-generator": "^0.2.2",
        "beyondcode/laravel-view-xray": "^1.0",
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^2.10",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^3.0",
        "lorisleiva/laravel-deployer": "^0.2.3",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~2.0",
        "nunomaduro/larastan": "dev-master",
        "phploc/phploc": "^4.0",
        "phpunit/phpunit": "~7.0",
        "sensiolabs/security-checker": "^4.1",
        "wnx/laravel-stats": "^1.7"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "format": [
            "vendor/bin/php-cs-fixer fix",
            "php artisan ide-helper:models -W",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:generate"
        ],
        "ide-helper": [
            "php artisan ide-helper:models -W",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:generate"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
